RunService.RenderStepped:Connect(function()
    -- 1. ‡∏°‡∏≠‡∏á‡∏î‡πâ‡∏≤‡∏ô‡∏´‡∏•‡∏±‡∏á (Back Detection)
    if Toggle.BackDetect and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local myRoot = LocalPlayer.Character.HumanoidRootPart
        local closestEnemy = nil
        local minDist = 50 -- ‡∏£‡∏∞‡∏¢‡∏∞‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö (‡∏õ‡∏£‡∏±‡∏ö‡πÑ‡∏î‡πâ)

        for _, p in pairs(Players:GetPlayers()) do
            if p ~= LocalPlayer and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
                local targetRoot = p.Character.HumanoidRootPart
                local directionToTarget = (targetRoot.Position - myRoot.Position).Unit
                local myForward = myRoot.CFrame.LookVector
                
                -- Dot Product < 0 ‡∏Ñ‡∏∑‡∏≠‡∏≠‡∏¢‡∏π‡πà‡∏î‡πâ‡∏≤‡∏ô‡∏´‡∏•‡∏±‡∏á
                local dot = myForward:Dot(directionToTarget)
                
                if dot < -0.2 then -- ‡∏¢‡∏¥‡πà‡∏á‡∏Ñ‡πà‡∏≤‡∏ô‡πâ‡∏≠‡∏¢‡∏¢‡∏¥‡πà‡∏á‡∏≠‡∏¢‡∏π‡πà‡∏´‡∏•‡∏±‡∏á‡∏ï‡∏£‡∏á‡πÜ
                    local dist = (targetRoot.Position - myRoot.Position).Magnitude
                    if dist < minDist then
                        minDist = dist
                        closestEnemy = p.Name
                    end
                end
            end
        end

        if closestEnemy then
            backWarn.Text = "‚ö†Ô∏è ‡∏î‡πâ‡∏≤‡∏ô‡∏´‡∏•‡∏±‡∏á: " .. closestEnemy
        else
            backWarn.Text = ""
        end
    else
        backWarn.Text = ""
    end
end)

